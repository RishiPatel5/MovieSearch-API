<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/main.css"> 
</head>

<body>

    <div class="content">
        <ul class="breadcrumbs">
            <li> <a href="/"> Dash</a> </li>
            <li> / </li>
            <li> Clubs </li>
        </ul>
        <h1> Clubs </h1>

        <section class="dataTable">
            <table class="data">
                <tr>
                    <th>Club ID</th>
                    <th>Club Name</th>
                    <th>Description</th>
                </tr>
                <% clubs.forEach(club => { %>
                    <tr>
                        <td><%= club.Club_ID %></td>
                        <td><%= club.Club_Name %></td>
                        <td><%= club.Club_Description %></td>
                    </tr>
                <% }); %>
            </table>
        </section>
    </div>

</body>

</html>
